	<event name="RVS_GENERIC_ENCOUNTER_AI_REGULAR_LIMIT" unique="true">
		<text load="RVS_TEXT_GENERIC_ENCOUNTER_AI"/>
		<ship load="RVS_SHIP_AI_FIGHT" hostile="true"/>
